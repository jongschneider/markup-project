extends layout

include mixins/_searchForms.pug

block content
  +searchForms()


  h1 Highest Scored Unique Item 
  table.table.table-striped.table-hover
    thead
      tr
        th Date
        th Key
        th File Name
        th Score
    tbody
      each row in max
        tr
          td #{row.score_runtime}
          td #{row.html_filenames_html_keys_html_keyname}
          td #{row.html_filenames_html_filename}
          td #{row.score}
        tr
