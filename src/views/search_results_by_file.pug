extends layout

include mixins/_searchForms.pug

block content
  +searchForms()

  h1 #{title}
  table.table.table-striped
    caption List of All Files
    thead
      tr
        th Date
        th Key
        th File Name
        th Score
    tbody
      each row in results
        tr
          td #{row.score_runtime}
          td #{row.html_filenames_html_keys_html_keyname}
          td #{row.html_filenames_html_filename}
          td #{row.score}
        tr

  table.table.table-striped
    caption Averages
    thead
      tr
        th File Name
        th Average Score
    tbody
      each row in avgs
        tr
          td #{row.html_filenames_html_filename}
          td #{row["AVG(score)"]}
        tr
